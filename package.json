{
  "name": "monorepo",
  "version": "0.0.0",
  "private": true,
  "keywords": [],
  "workspaces": [
    "packages/*",
    "platform/*",
    "apps/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean && rm -rf node_modules",
    "dev": "turbo run dev --parallel",
    "docs": "turbo run docs --filter=@monorepo/docs",
    "lint": "turbo run lint",
    "lint-staged": "npm exec lint-staged",
    "prepare": "husky install",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "eslint --ext .js,.jsx,.ts,.tsx --fix --max-warnings=0",
    "**/*.{js,jsx,json,yml,yaml,css,less,scss,ts,tsx,md,graphql}": "prettier --write"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.9",
    "@mui/material": "^5.11.10",
    "@mui/styles": "^5.11.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^5.3.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^18.14.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "eslint": "^8.34.0",
    "eslint-config-monorepo": "*",
    "husky": "^8.0.3",
    "jsdom": "latest",
    "lint-staged": "^13.1.2",
    "plop": "^3.1.2",
    "prettier": "^2.8.4",
    "turbo": "^1.8.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.3",
    "vitest": "^0.28.5"
  }
}
